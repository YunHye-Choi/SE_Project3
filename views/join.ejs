    <div class="content">
      <div id="comments">
      <p class = "sign">회원가입</p>
        <form id= "joinForm" action="/join" method="post">
          <div class="join_Form">
            <tr>
              <th>아이디</th>
              <td><input type="text" name="id" id="id" size="12" maxlength="12"></td>
            </tr>          
          </div>
          <div class="join_Form">
            <label for="passwd">비밀번호 <span>*</span></label>
            <input type="password" name="passwd" id="passwd">
          </div>
          <div class="join_Form">
            <label for="passwdcheck">비밀번호 확인 <span>*</span></label>
            <input type="password" name="passwdcheck" id="passwdcheck">
          </div>
          <div class="join_Form">
            <label for="name">이름<span>*</span></label>
            <input type="text" name="name" id="name">
          </div>
          <div class="join_Form">
            <label for="email">이메일</label>
            <input type="email" name="email" id="email">
          </div>
          <div class="join_Form">
            <label for="phoneNo">휴대전화<span>*</span></label>
            <input type="tel" name="phoneNo" id="phoneNo">
          </div>
          <div class="join_Form">
            <label for="yy">생년월일</label></h3>
              <div id="bir_wrap">
                <!-- BIRTH_YY -->
                <div id="bir_yy">
                    <input type="text" id="yy" class="int" maxlength="4" placeholder="년(4자)">
                </div>

                <!-- BIRTH_MM -->
                <div id="bir_mm">
                    <input type="text" id="mm" class="int" maxlength="2" placeholder="월">
                </div>

                <!-- BIRTH_DD -->
                <div id="bir_dd">
                    <input type="text" id="dd" class="int" maxlength="2" placeholder="일">
                </div>

              </div>
              <span class="error_next_box"></span>
          </div>
          <div class="join_Form">
            <label for="who">목적 <span>*</span></label>
            <input type="radio" name="who" id="who_1" checked = "checked" value="who_1"/> 구매자
            <input type="radio" name="who" id="who_2" value="who_2"/> 판매자
          </div>
          <div class="join_Form">
            <input type="button" id="join" name="join" value="회원가입">
          </div>
        </form>
      </div>
    </div>
    <script>
      $(function(){
          $("#join").click(
              function(){
                  
                  if($("#id").val()=="")
                  {
                      alert("아이디를 꼭 입력 하세요");
                      $("#id").focus();
                      return;
                  }
                      $("#joinForm").submit();          
                  });
      });
    </script>
